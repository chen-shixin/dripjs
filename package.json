    "@types/ws": "6.0.1",
    "@zerollup/ts-transform-paths": "1.7.0",
    "coveralls": "3.0.3",
    "import-sort": "6.0.0",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module-alias": "1.0.4",
    "jest": "24.1.0",
    "markdownlint": "0.12.0",
    "markdownlint-cli": "0.14.0",
