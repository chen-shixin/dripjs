{
  "name": "dripjs",
  "version": "0.0.14",
  "description": "一个帮助交易者建立全面完善的交易系统的量化开源平台",
  "engines": {
    "node": ">= 10.15.0",
    "npm": ">= 6.4.1",
    "yarn": ">= 1.12.3"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "bdd": "yarn jest --watch --onlyChanged",
    "build:prod": "yarn build:preprocess && yarn tsc:app && yarn build:postprocess",
    "build:preprocess": "rm -rf dist",
    "build:postprocess": "cp package.json README.md README_EN.md dist",
    "coverage": "jest -c jest.config.js --collectCoverage",
    "ci:coverage": "jest -c jest.config.js --coverage --coverageReporters=text-lcov | coveralls",
    "dev:bitmex": "ts-node --files -r tsconfig-paths/register ./projects/intelligence/main.ts",
    "format": "yarn format:ts && yarn format:md",
    "format:md": "prettier --config .prettierrc --write '{docs,modules,projects}/**/*.md' --parser markdown",
    "format:ts": "yarn format:ts:import && prettier --config .prettierrc --write '{modules,projects}/**/*.ts'",
    "format:ts:import": "import-sort --write '{modules,projects}/**/*.ts'",
    "jest": "jest -c jest.config.js",
    "lint": "yarn lint:md && yarn lint:ts",
    "lint:md": "markdownlint **/*.md",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json",
    "pub": "yarn build:prod && npm publish dist",
    "test": "yarn jest --runInBand",
    "tsc:app": "ttsc -p tsconfig.app.json",
    "tsc:spec": "ttsc -p tsconfig.spec.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlq4863947/dripjs.git"
  },
  "keywords": [
    "trading platform",
    "algorithmic",
    "algotrading",
    "altcoin",
    "altcoins",
    "api",
    "arbitrage",
    "backtest",
    "backtesting",
    "bitcoin",
    "dripjs",
    "drip",
    "crypto",
    "digital currency",
    "exchange",
    "exchanges",
    "framework",
    "invest",
    "investing",
    "investor",
    "library",
    "light",
    "litecoin",
    "ltc",
    "market",
    "market data",
    "markets",
    "merchandise",
    "merchant",
    "minimal",
    "ohlcv",
    "order",
    "orderbook",
    "order book",
    "price",
    "price data",
    "pricefeed",
    "private",
    "public",
    "ripple",
    "strategy",
    "ticker",
    "tickers",
    "toolkit",
    "trade",
    "trader",
    "trading",
    "usd",
    "volume",
    "xbt",
    "xrp",
    "zec",
    "BitMEX"
  ],
  "author": "zlq4863947@gmail.com",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/zlq4863947/dripjs/issues"
  },
  "homepage": "https://github.com/zlq4863947/dripjs#readme",
  "devDependencies": {
    "@types/jest": "24.0.6",
    "@types/qs": "6.5.1",
    "@types/ws": "6.0.1",
    "@zerollup/ts-transform-paths": "1.7.0",
    "coveralls": "3.0.2",
    "import-sort": "6.0.0",
    "import-sort-cli": "5.2.0",
    "import-sort-parser-typescript": "5.0.0",
    "import-sort-style-module-alias": "1.0.4",
    "jest": "23.6.0",
    "markdownlint": "0.12.0",
    "markdownlint-cli": "0.13.0",
    "prettier": "1.16.4",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.12.1",
    "tslint-eslint-rules": "5.4.0",
    "ttypescript": "1.5.6",
    "typescript": "3.3.3",
    "vrsource-tslint-rules": "6.0.0",
    "yarn": "1.13.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dripjs-common": "^0.1.0",
    "dripjs-types": "^0.1.1",
    "dotenv": "^6.2.0",
    "qs": "^6.6.0",
    "rxjs": "^6.4.0",
    "ws": "^6.1.3"
  }
}
